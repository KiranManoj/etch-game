diff --git a/main.py b/main.py
new file mode 100644
--- /dev/null	(date 1660030553369)
+++ b/main.py	(date 1660030553369)
@@ -0,0 +1,34 @@
+from turtle import Turtle, Screen
+
+tim = Turtle()
+screen = Screen()
+
+
+def move_forward():
+    tim.forward(30)
+
+
+def move_back():
+    tim.backward(30)
+
+
+def turn_right():
+    tim.right(15)
+
+
+def turn_left():
+    tim.left(15)
+
+
+def clear():
+    screen.reset()
+
+
+screen.listen()
+screen.onkey(key="w", fun=move_forward)
+screen.onkey(key="s", fun=move_back)
+screen.onkey(key="d", fun=turn_right)
+screen.onkey(key="a", fun=turn_left)
+screen.onkey(key="c", fun=clear)
+
+screen.exitonclick()
